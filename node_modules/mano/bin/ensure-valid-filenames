#!/usr/bin/env node

'use strict';

Error.stackTraceLimit = Infinity;

// Prevent EMFILE errors
require('fs2/descriptors-handler')();

require('../scripts/find-broken-filenames')(process.cwd(),
	{ log: process.stdout.write.bind(process.stdout) }).done(function (result) {
		if (!result.length) return;
	process.exit(1);
	});
