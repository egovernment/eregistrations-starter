#!/usr/bin/env node

// Generates fast loading client models for this app
// This module was initially generated by mano/scripts/generate-app-client-model-script

'use strict';

Error.stackTraceLimit = Infinity;

var resolve  = require('path').resolve
  , i18n     = require('i18n2')
  , mano     = require('mano')
  , generate = require('mano/scripts/generate-app-client-model')
  , env      = require('../../../env');

mano.i18n = i18n((env.i18n === false) ? null : require('../../../i18n'));

generate(require('../client/model'), resolve(__dirname, '../')).done();

